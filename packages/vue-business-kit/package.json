{
  "name": "vue-business-kit",
  "version": "0.0.1",
  "description": "Ready-to-use component library based on Vue3 + TypeScript + Element Plus + Lodash",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./global": {
      "types": "./types/global.d.ts"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.es.js",
      "require": "./dist/utils.cjs.js"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "generate:icons": "tsx scripts/generate-icons.ts",
    "generate:types": "tsx scripts/generate-component-types.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dongchen-xie/vue-business-kit"
  },
  "homepage": "https://dongchen-xie.github.io/vue-business-kit/en-US/",
  "keywords": [
    "vue",
    "vue3",
    "typescript",
    "element-plus",
    "lodash",
    "library"
  ],
  "author": "dongchen_xie",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.12.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@iconify-json/tabler": "^1.2.23",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "*",
    "@vitejs/plugin-vue": "^5.0.3",
    "typescript": "^5.2.2",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.3.0",
    "vue": "^3.5.13",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": ">= 18.0.0",
    "pnpm": ">=10.0.0"
  }
}
